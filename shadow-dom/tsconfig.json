{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": false,
    "preserveConstEnums": false,
    "sourceMap": false,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/auth"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}